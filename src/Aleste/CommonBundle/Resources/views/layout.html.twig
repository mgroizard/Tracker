<html>
    <head>
    {% if  'MSIE' in app.request.server.get('HTTP_USER_AGENT') %}
     <meta http-equiv="X-UA-Compatible" content="IE=edge;chrome=1" />
    {% endif %}
    <title>.: Tracker :.</title>
    {% block stylesheets %}
     <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
     <link href="{{ asset('bundles/tracker/css/bootstrap.min.css') }}" rel="stylesheet" media="screen">
     <link href="{{ asset('bundles/common/css/loteria.css') }}" rel="stylesheet">
    {% endblock %}
    {% block otherStylesheets %}{% endblock %}

    {% block javascripts %}
    <script src="{{ asset('bundles/tracker/js/jquery.min.js')}}"></script>
    <script src="{{ asset('bundles/tracker/js/bootstrap.min.js')}}"></script>
 
    <link rel="stylesheet" href="{{ asset('bundles/common/js/jquery-ui-themes-1.10.4/themes/smoothness/jquery-ui.css')}}">
    <script src="{{ asset('bundles/common/js/jquery-ui-1.10.4/ui/minified/jquery-ui.min.js') }}"></script>
    <script src="{{ asset('bundles/common/js/jquery-ui-1.10.4/ui/i18n/jquery-ui-i18n.js') }}"></script>

    <script type="text/javascript">

        (function($) {
	    // mio
             $(document).ready(function(){
                $('body').delegate('.datepicker', 'focusin', function(){$(this).datepicker({'changeMonth': true,'changeYear': true});});
             });
            })(jQuery);
    </script>
    <script src="{{ asset('bundles/common/js/jquery-ui-1.10.4/ui/i18n/jquery.ui.datepicker-'~app.request.locale~'.js') }}"></script>
    
  {% endblock %}
  {% block otherJavascripts %}{% endblock %}
  </head>
  <body class="preview" id="top" data-spy="scroll" data-target=".subnav" >

    {% block cabecera %}{% endblock %}
    
    {% block menu %}
     {#
     {% include "CommonBundle:CommonViews:menu.html.twig" %}
     #}
    {% endblock %}
    <div class="container">
       {% include 'CommonBundle:CommonViews:flash.messages.html.twig' %}
    </div>
    
    {% block titulo %}{% endblock %}
    {% block filtro %}{% endblock %}
    {% block contenido %}
        {% block ajax %}{% endblock %}
    {% endblock %}
    {% block footer %}{% endblock %}
  </body>
</html>